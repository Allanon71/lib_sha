Function _XOR_BYTE(x, y)
  x = x & ToNumber("11111111", 2)
  y = y & ToNumber("11111111", 2)
  Local r = x + y - 2 * AND_of_two_bytes[x + y * 256]
  Return(r)
EndFunction

Function _HEX(x)
  ;Return(FormatStr("%08x", x % 4294967296))
